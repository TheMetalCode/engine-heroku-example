{
  "origins": [
    {
      "http": {
        "url": "https://sample-graphql-ruby.herokuapp.com/graphql",
        "overrideRequestHeaders": {
          "Host": "sample-graphql-ruby.herokuapp.com"
        }
      }
    }
  ],
  "frontends": [
    {
      "host": "0.0.0.0",
      "portFromEnv": "PORT",
      "endpoint": "/graphql"
    }
  ],
  "stores": [
    {
      "name": "embeddedCache",
      "inMemory": {
        "cacheSize": 10485760
      }
    },
    {
      "name": "pq",
      "inMemory": {
        "cacheSize": 10485760
      }
    }
  ],
  "sessionAuth": {
    "store": "embeddedCache",
    "header": "Authorization"
  },
  "queryCache": {
    "publicFullQueryStore": "embeddedCache",
    "privateFullQueryStore": "embeddedCache"
  },
  "persistedQueries": {
    "store": "pq"
  },
  "logging": {
    "level": "DEBUG"
  }
}
